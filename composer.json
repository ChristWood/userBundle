{
  "name": "capersys/user-bundle",
  "description": "Symfony User manager",
  "keywords": [
    "User management"
  ],
  "homepage": "https://github.com/capersys/user-bundle",
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Ramazan APAYDIN",
      "homepage": "https://github.com/appaydin"
    },
    {
      "name": "Christophe",
      "homepage": "https://github.com/capersys"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=7.2.5",
    "doctrine/doctrine-bundle": "^2.0",
    "doctrine/orm": "^2.3",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "symfony/console": "^5.2",
    "symfony/dependency-injection": "^5.2",
    "symfony/dotenv": "^5.2",
    "symfony/event-dispatcher": "^5.2",
    "symfony/flex": "^1.3.1",
    "symfony/form": "^5.2",
    "symfony/framework-bundle": "^5.2",
    "symfony/mailer": "^5.2",
    "symfony/security-core": "^5.2",
    "symfony/security-http": "^5.2",
    "symfony/translation": "^5.2",
    "symfony/validator": "^5.2",
    "symfony/yaml": "^5.2",
    "twig/twig": "^3.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.2",
    "symfony/phpunit-bridge": "^5.2"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "christwood\\UserBundle\\": ""
    }
  },
  "autoload-dev": {
    "psr-4": {
      "christwood\\UserBundle\\Tests\\": "tests/"
    }
  },
  "replace": {
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php72": "*"
  },
  "conflict": {
    "phpunit/phpunit": "<5.4.3"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "5.2.*"
    }
  }
}
